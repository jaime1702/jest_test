{
  "verbose": true,
  "testTimeout": 10000,
  "moduleDirectories": ["node_modules", "jest_test"],
  "setupFilesAfterEnv": ["jest-extended/all"],
  "reporters": [
    "default",
    [
      "jest-html-reporters",
      {
        "publicPath": "./html-report",
        "filename": "report.html",
        "expand": true,
        "openReport": false,
        "pageTitle": "Jest Test"
      }
    ]
  ]
}
